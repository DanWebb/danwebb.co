{
  "name": "danwebb-portfolio",
  "version": "0.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "serve": "serve dist -p 3000"
  },
  "xo": {
    "extends": "xo-react",
    "parser": "babel-eslint",
    "globals": [
      "window",
      "document",
      "fetch"
    ],
    "rules": {
      "import/no-unresolved": 0,
      "camelcase": 0,
      "new-cap": [
        "error",
        {
          "capIsNew": false
        }
      ]
    }
  },
  "babel": {
    "extends": "react-static/.babelrc",
    "plugins": [
      "babel-plugin-styled-components"
    ]
  },
  "dependencies": {
    "axios": "0.18.0",
    "chokidar": "2.0.3",
    "fetch-jsonp": "1.1.3",
    "highlightjs": "9.10.0",
    "marked": "0.3.19",
    "popmotion": "8.1.22",
    "prop-types": "^15",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "^3",
    "react-pose": "1.5.2",
    "react-router": "^4.2.0",
    "react-static": "5.8.1",
    "styled-components": "^3"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-plugin-styled-components": "^1.3.0",
    "camelcase": "5.0.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-config-xo-react": "0.13.0",
    "htmr": "0.6.3",
    "jdown": "0.5.1",
    "serve": "^6.1.0",
    "xo": "^0.20"
  }
}
